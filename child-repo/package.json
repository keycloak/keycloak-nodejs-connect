    "jsdoc": "^3.4.0",
    "jshint": "^2.9.2",
    "nock": "^8.0.0",
    "nsp": "*",
    "semistandard": "^7.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
